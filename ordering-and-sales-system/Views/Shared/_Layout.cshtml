<body>
    <div>
        <nav class="navbar">
            <div class="logo">
                <img src="~/images/logo.svg">
            </div>

            <div class="nav-items">
                <ul class="nav-links">
                    <li class="item-link active">
                        <a href="#">
                            <span class="text1 nav-text">About</span>
                        </a>
                    </li>

                    <li class="item-link active">
                        <a href="#">
                            <span class="text nav-text">Administrator</span>
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>


    <div class="content">
