                                <defs>
                                  <clipPath id="clip0_1222_37202">
                                    <rect width="14" height="14" fill="white"/>
                                  </clipPath>
                                </defs>
                              </svg>
                            <span class="text nav-text">Transaction</span>
                            <svg id="dropdown" xmlns="http://www.w3.org/2000/svg" width="11" height="11" viewBox="0 0 7 4" fill="none">
                                <path d="M6 1L3.5 3.5L1 1"  stroke-linecap="round" stroke-linejoin="round"/>
                              </svg>
                        </a>
                        <ul class="submenu">
                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="PendingOrders">
                                    <span class="text">Pending Orders</span>
                                </a>
                            </li>

                            <li>
                                <a asp-area="" asp-controller="Home" asp-action="TransactionHistory">
                                    <span class="text">Transaction History</span>
                                </a>
                            </li>
                        </ul>
                    </li>


